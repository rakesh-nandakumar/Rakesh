{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:admin": "cd admin && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:admin\" --names \"NEXT,ADMIN\" --prefix-colors \"cyan,magenta\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser npm run build",
    "upload-to-supabase": "node scripts/upload-to-supabase.js",
    "test-storage": "node scripts/test-file-storage.js",
    "test-pwa": "node test/pwa-system.test.js",
    "fix-bucket": "node scripts/fix-bucket.js",
    "migrate-assets": "node scripts/migrate-assets-to-supabase.js",
    "cleanup-assets": "node scripts/cleanup-migrated-assets.js --confirm"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@next/bundle-analyzer": "^15.5.4",
    "@supabase/supabase-js": "^2.81.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "framer-motion": "^12.15.0",
    "glob": "^11.0.3",
    "mime-types": "^3.0.1",
    "next": "^15.5.6",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.6",
    "pg": "^8.16.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-feather": "^2.0.10",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.57.0",
    "react-markdown": "^10.1.0",
    "reading-time": "^1.5.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "swr": "^2.3.6",
    "web-vitals": "^5.1.0",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^4"
  }
}
